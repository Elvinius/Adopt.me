{
	"extends": ["eslint:recommended", "prettier", "prettier/react"],
	"rules": {
		"react/prop-types": 0,
		"no-console": 1,
		"react-hooks/rules-of-hooks":2,
		"react-hooks/exhaustive-deps":1
	},
	"plugins": ["react-hooks"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeature": {
			"jsx":true
		}
	},
	"env": {
		"es6":true,
		"browser": true,
		"node": true
	}
}